{
    "version": 2,
    "builds":[
        {
            
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "headers":[
        {
            "source":"/api/(.*)",
            "headers":[
                {"key":" Access-Control-Allow-Origin", "value": "http://localhost:5173"},
                {"key":" Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS"},
                {"key":" Access-Control-Allow-Headers", "value": "ontent-Type, Authorization"},

            ]
        }
    ]
}